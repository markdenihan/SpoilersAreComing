{
   "browser_action": {
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "css": [ "/css/style.css" ],
      "js": [ "jquery-3.3.1.min.js", "content.js" ],
      "matches": [ "*://*/*" ]
   } ],
   "description": "This GoT Spoiler Blocker protects you from the Game of Thrones Season 8 spoilers that are out there anywhere on the Internet.",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "manifest_version": 2,
   "name": "Spoilers Are Coming",
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.801",
   "web_accessible_resources": [ "/*" ]
}
